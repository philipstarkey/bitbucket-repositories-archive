tories/cbillington/qtutils/commit/98a06a7588989c90d8b7a02d18cb31731fb2be46.json"}, "comments": {"href": "data/repositories/cbillington/qtutils/commit/98a06a7588989c90d8b7a02d18cb31731fb2be46/comments_page=1.json"}, "patch": {"href": "https://api.bitbucket.org/2.0/repositories/cbillington/qtutils/patch/98a06a7588989c90d8b7a02d18cb31731fb2be46"}, "html": {"href": "#!/cbillington/qtutils/commits/98a06a7588989c90d8b7a02d18cb31731fb2be46"}, "diff": {"href": "https://api.bitbucket.org/2.0/repositories/cbillington/qtutils/diff/98a06a7588989c90d8b7a02d18cb31731fb2be46"}, "approve": {"href": "https://api.bitbucket.org/2.0/repositories/cbillington/qtutils/commit/98a06a7588989c90d8b7a02d18cb31731fb2be46/approve"}, "statuses": {"href": "data/repositories/cbillington/qtutils/commit/98a06a7588989c90d8b7a02d18cb31731fb2be46/statuses_page=1.json"}}, "author": {"raw": "chrisjbillington", "type": "author"}, "parents": [{"hash": "ce633e06d4e2b60d0cc92f666a501302b27b1bcf", "type": "commit", "links": {"self": {"href": "data/repositories/cbillington/qtutils/commit/ce633e06d4e2b60d0cc92f666a501302b27b1bcf.json"}, "html": {"href": "#!/cbillington/qtutils/commits/ce633e06d4e2b60d0cc92f666a501302b27b1bcf"}}}], "date": "2019-03-28T19:56:28+00:00", "message": "Upscale icons for better HiDPI support.\n\nCustom icons were upscaled by re-exporting source svgs at higher resolution.\n\nFugue icons upscaled to 32x32 by simple pixel doubling. This way we are not\nrelying on OS scaling or on Qt to scale the icons, which may scale with\ninterpolation that looks even worse considering icons are pixel-art.\n\nThe script to perform the upscaling is included in the repository, and\nthe original 16x16 icons kept.\n\nThis is a decent stop-gap to look OK on high iDPI screens until an icon set as\ncomprehensive as fugue exists at higher resolution (I cannot find anything\ncomparable at the moment).\n\nIncluding icons in multiple sizes was an option I explored, but it does not work\nwell with Qt designer - it does not choose the correct icon size from .ico\nfiles. Including an excessively large icon and letting Qt downscale is the\ncurrent best practice (unfortunately). I would have liked to upscale the icons\nusing clever pixel-art scaling filters, but then they look terrible when\nQt *downscales* them fot a standard DPI screen. So scaling by pixel doubling\nis the lesser evil for the moment.", "type": "commit"}}, {"heads": [{"hash": "a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3", "type": "commit", "links": {"self": {"href": "data/repositories/cbillington/qtutils/commit/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3.json"}, "html": {"href": "#!/cbillington/qtutils/commits/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3"}}}], "name": "issue5", "links": {"commits": {"href": "data/repositories/cbillington/qtutils/commits/issue5_page=1.json"}, "self": {"href": "data/repositories/cbillington/qtutils/refs/branches/issue5.json"}, "html": {"href": "#!/cbillington/qtutils/branch/issue5"}}, "default_merge_strategy": "merge_commit", "merge_strategies": ["merge_commit"], "type": "named_branch", "target": {"hash": "a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3", "repository": {"links": {"self": {"href": "data/repositories/cbillington/qtutils.json"}, "html": {"href": "#!/cbillington/qtutils"}, "avatar": {"href": "data/bytebucket.org/ravatar/{0c98b491-bfb0-4ce7-9729-bc4606436bde}ts=python"}}, "type": "repository", "name": "qtutils", "full_name": "cbillington/qtutils", "uuid": "{0c98b491-bfb0-4ce7-9729-bc4606436bde}"}, "links": {"self": {"href": "data/repositories/cbillington/qtutils/commit/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3.json"}, "comments": {"href": "data/repositories/cbillington/qtutils/commit/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3/comments_page=1.json"}, "patch": {"href": "https://api.bitbucket.org/2.0/repositories/cbillington/qtutils/patch/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3"}, "html": {"href": "#!/cbillington/qtutils/commits/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3"}, "diff": {"href": "https://api.bitbucket.org/2.0/repositories/cbillington/qtutils/diff/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3"}, "approve": {"href": "https://api.bitbucket.org/2.0/repositories/cbillington/qtutils/commit/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3/approve"}, "statuses": {"href": "data/repositories/cbillington/qtutils/commit/a36e5b63e73e38ba04c201d5b6a5e6e9f38ed2a3/statuses_page=1.json"}}, "author": {"raw": "PhyNerd <janwerkmann@gmail.com>", "type": "author"}, "parents": [{"hash": "73144e1521f16570a85189c520ad1aa5b4f9d616", "type": "commit", "links": {"self": {"href": "data/repositories/cbillington/qtutils/commit/73144e1521f16570a85189c520ad1aa5b4f9d616.json"}, "html": {"href": "#!/cbillington/qtutils/commits/73144e1521f16570a85189c520ad1aa5b4f9d616"}}}], "date": "2017-11-09T19:20:34+00:00", "message": "map QtCore.QItemSelectionModel to QtGui.QItemSelectionModel in PyQt4", "type": "commit"}}], "page": 1, "size": 8}